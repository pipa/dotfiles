# General aliases
alias ll='ls -la' # List files with details
alias la='ls -A'  # List all files including hidden
alias l='ls -CF'  # List files in columns
alias grep='grep --color=auto' # Colorize grep output

# Shortcuts
alias dev="cd ~/Developer"

# Navigation aliases
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'

